<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>QUÉT MỠ</title>
    <link rel="stylesheet" href="~/adminlte-master/plugins/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="~/adminlte-master/dist/css/adminlte.min.css">
    <script src="~/adminlte-master/plugins/jquery/jquery.min.js"></script>
    <script src="~/adminlte-master/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/sweetalert2/sweetalert2.min.js"></script>
    <link href="~/lib/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
</head>
<body class="hold-transition sidebar-mini dark-mode">
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand navbar-white dark-mode">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item dropdown">
                    <button class="btn bg-lightblue">
                        @ViewBag.tenpage
                    </button>
                </li>
            </ul>
        </nav>
        <aside class="main-sidebar sidebar-light-primary elevation-4">
            <a href="#" class="brand-link" style="text-align:center">
                <span class="brand-text font-weight-light">CEV SCANNER </span>
                <i class="fa fa-cog fa-spin fa-fw"></i>

            </a>
            <div class="sidebar">
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item">
                            <a asp-action="pd1" asp-controller="xuatline" class="nav-link bg-lightblue">
                                <i style="color:darkorange" class="nav-icon fas fa-upload"></i>
                                <p>
                                    XUẤT LINE PD1
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-action="pd2" asp-controller="xuatline" class="nav-link bg-lightblue">
                                <i style="color:darkorange" class="nav-icon fas fa-upload"></i>
                                <p>
                                    XUẤT LINE PD2
                                </p>
                            </a>
                        </li>

                    </ul>
                </nav>
            </div>
        </aside>
        <div class="content-wrapper ">
            <br />
            <section class="content">
                @RenderBody()
            </section>
        </div>
    </div>
    <script src="~/adminlte-master/plugins/jquery/jquery.min.js"></script>
    <script src="~/adminlte-master/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/adminlte-master/dist/js/adminlte.min.js"></script>
</body>
</html>

